{
  "name": "marcel-mathias-nolte/contao-photo-blog",
  "description": "A blogging / news system for Contao",
  "keywords": ["news", "blog"],
  "type": "contao-module",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Leo Feyer",
      "homepage": "https://github.com/leofeyer"
    },
    {
      "name": "Contao Community",
      "homepage": "https://contao.org/contributors"
    },
    {
      "name": "Marcel Mathias Nolte",
      "homepage": "https://github.com/marcel-mathias-nolte"
    },
    {
      "name": "Christoph Wiechert",
      "email": "wio@psitrax.de",
      "homepage": "http://4wardmedia.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2",
    "contao/core-bundle": "4.*",
    "contao-community-alliance/composer-plugin": "^2.4 || ^3.0",
    "menatwork/contao-multicolumnwizard-bundle": "~3"
  },
  "support": {
    "issues": "https://github.com/marcel-mathias-nolte/contao-photoblog/issues",
    "source": "https://github.com/marcel-mathias-nolte/contao-photoblog"
  },
  "autoload": {
    "psr-4": {
      "MarcelMathiasNolte\\ContaoPhotoBlogBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "MarcelMathiasNolte\\ContaoPhotoBlogBundle\\ContaoManager\\Plugin"
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "contao-community-alliance/composer-plugin": true
    }
  }
}
